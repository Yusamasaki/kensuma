<% provide(:title, "車両登録") %>
<% provide(:btn_text, "登録") %>

<div class="card">
  <%= form_with model: @car, url: users_cars_path, method: :post, local: true do |f| %>
    <div class="card-body">
      <%= render partial: "form", locals: { f: f, car: @car } %>
    </div>
    <div class="card-footer">
      <%= f.submit yield(:btn_text), class: "btn btn-md btn-block btn-primary" %>
    </div>
  <% end %>
</div>
